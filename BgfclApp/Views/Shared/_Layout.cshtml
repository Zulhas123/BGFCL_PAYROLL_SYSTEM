<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - BGFSC-PAYROLL</title>

    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="~/css/bootstrap.min.css">
    <!-- Fontawesome CSS -->
    <link rel="stylesheet" href="~/css/font-awesome.min.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.3/css/all.min.css">

    <!-- Data Table CSS -->
    <link rel="stylesheet" href="~/css/datatables.min.css">
    <link rel="stylesheet" href="https://cdn.datatables.net/buttons/2.3.6/css/buttons.dataTables.min.css">
    <!-- Lineawesome CSS -->
    <link rel="stylesheet" href="~/css/line-awesome.min.css">
    <!-- Toastr CSS -->
    <link rel="stylesheet" href="~/css/toastr.min.css">
    <!-- Select2 CSS -->
    <link rel="stylesheet" href="~/css/select2.min.css">
    <!-- Date Time Picker CSS -->
    <link rel="stylesheet" href="~/css/bootstrap-datetimepicker.min.css">
    <link rel="stylesheet" href="https://bossanova.uk/jspreadsheet/v4/jexcel.css" type="text/css" />
    <link rel="stylesheet" href="https://jsuites.net/v4/jsuites.css" type="text/css" />
    <!-- Main CSS -->
    <link rel="stylesheet" href="~/css/style.css">

    <!-- Include Chosen CSS -->
    <link href="https://cdn.jsdelivr.net/npm/chosen-js@latest/chosen.min.css" rel="stylesheet" />

    <!-- Section Wise CSS -->
    @await RenderSectionAsync("Styles",false)
</head>
<body>
    <!-- Main Wrapper -->
    <div class="main-wrapper">
        <!-- Header -->
        <partial name="_Header"/>
        <!-- /Header -->

        <!-- Sidebar -->
        <partial name="_Sidebar"/>
        <!-- /Sidebar -->

        <!-- Page Wrapper -->
        <div class="page-wrapper">
            <!-- Page Content -->
            <div class="content container-fluid">
                @RenderBody()
                <!-- Sidebar -->
                <partial name="_Modal" />
                <!-- /Sidebar -->
            </div>
            <!-- /Page Content -->
        </div>
        <!-- /Page Wrapper -->
    </div>
    <!-- /Main Wrapper -->

    <!-- jQuery -->
    <script src="~/lib/jquery/dist/jquery-3.7.1.min.js"></script>
    <!-- Bootstrap Core JS -->
    <script src="~/js/popper.min.js"></script>
    <script src="~/js/bootstrap.min.js"></script>
    <!-- Data Table JS -->
    <script src="~/js/datatables.min.js"></script>

    <script src="https://cdn.datatables.net/buttons/2.3.6/js/dataTables.buttons.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jszip/3.10.1/jszip.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/pdfmake/0.2.7/pdfmake.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/pdfmake/0.2.7/vfs_fonts.js"></script>
    <script src="https://cdn.datatables.net/buttons/2.3.6/js/buttons.html5.min.js"></script>
    <script src="https://cdn.datatables.net/buttons/2.3.6/js/buttons.print.min.js"></script>



    <!-- Slimscroll JS -->
    <script src="~/js/jquery.slimscroll.min.js"></script>
    <!-- Toastr JS -->
    <script src="~/js/toastr.min.js"></script>
    <!-- Select2 JS -->
    <script src="~/js/select2.min.js"></script>
    <!-- Moment JS -->
    <script src="~/js/moment.min.js"></script>
    <!-- Date Time Picker JS -->
    <script src="~/js/bootstrap-datetimepicker.min.js"></script>
    <script src="https://bossanova.uk/jspreadsheet/v4/jexcel.js"></script>
    <script src="https://jsuites.net/v4/jsuites.js"></script>
    <!-- Common JS -->
    <script src="~/js/common.js"></script>
    <!-- Custom JS -->
    <script src="~/js/app.js"></script>

    <!-- Include Chosen JS -->
    <script src="https://cdn.jsdelivr.net/npm/chosen-js@latest/chosen.jquery.min.js"></script>
    <!-- Section Wise JS -->
    @await RenderSectionAsync("Scripts",false)
</body>
</html>
