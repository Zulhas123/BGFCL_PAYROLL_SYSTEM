                          <Paragraphs>
                            <Paragraph>
                              <TextRuns>
                                <TextRun>
                                  <Value>=Format(
    Sum(
        IIf(IsNothing(Fields!BasicSalary.Value), 0, Fields!BasicSalary.Value) + 
        IIf(IsNothing(Fields!LikeBasic.Value), 0, Fields!LikeBasic.Value) + 
        IIf(IsNothing(Fields!PersonalSalary.Value), 0, Fields!PersonalSalary.Value) + 
        IIf(IsNothing(Fields!ArrearSalary.Value), 0, Fields!ArrearSalary.Value) + 
        IIf(IsNothing(Fields!OtherSalary.Value), 0, Fields!OtherSalary.Value) + 
        IIf(IsNothing(Fields!LunchAllow.Value), 0, Fields!LunchAllow.Value) + 
        IIf(IsNothing(Fields!TiffinAllow.Value), 0, Fields!TiffinAllow.Value) + 
        IIf(IsNothing(Fields!SpecialBenefit.Value), 0, Fields!SpecialBenefit.Value) + 
        IIf(IsNothing(Fields!HouseRentAllow.Value), 0, Fields!HouseRentAllow.Value) + 
        IIf(IsNothing(Fields!FMAllow.Value), 0, Fields!FMAllow.Value) + 
        IIf(IsNothing(Fields!WashAllow.Value), 0, Fields!WashAllow.Value) + 
        IIf(IsNothing(Fields!EducationalAllow.Value), 0, Fields!EducationalAllow.Value) + 
        IIf(IsNothing(Fields!FieldRiskAllow.Value), 0, Fields!FieldRiskAllow.Value) + 
        IIf(IsNothing(Fields!ChargeAllow.Value), 0, Fields!ChargeAllow.Value) + 
        IIf(IsNothing(Fields!DAidAllow.Value), 0, Fields!DAidAllow.Value) + 
        IIf(IsNothing(Fields!DeputationAllow.Value), 0, Fields!DeputationAllow.Value) + 
		IIf(IsNothing(Fields!CME.Value), 0, Fields!CME.Value) + 
        IIf(IsNothing(Fields!OtherAllow.Value), 0, Fields!OtherAllow.Value)
    ) 
    - 
    Sum(
        IIf(IsNothing(Fields!RevenueStamp.Value), 0, Fields!RevenueStamp.Value) + 
        IIf(IsNothing(Fields!WelfareFund.Value), 0, Fields!WelfareFund.Value) + 
        IIf(IsNothing(Fields!OfficerClub.Value), 0, Fields!OfficerClub.Value) + 
        IIf(IsNothing(Fields!OfficerAssociation.Value), 0, Fields!OfficerAssociation.Value) + 
        IIf(IsNothing(Fields!ProvidentFund.Value), 0, Fields!ProvidentFund.Value) + 
        IIf(IsNothing(Fields!MedicalFund.Value), 0, Fields!MedicalFund.Value) + 
        IIf(IsNothing(Fields!TMBill.Value), 0, Fields!TMBill.Value) + 
